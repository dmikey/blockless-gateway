		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@blockless/gateway-core": "workspace:*",
		"@blockless/gateway-ui": "workspace:*",
		"@fastify/cors": "^8.2.1",
		"@fastify/env": "^4.2.0",
		"@fastify/jwt": "^7.2.0",
		"@fastify/multipart": "^7.7.3",
		"@ipld/car": "^5.2.5",
		"@ucanto/core": "^9.0.1",
		"@ucanto/principal": "^9.0.0",
		"@web3-storage/w3up-client": "^12.0.0",
		"dotenv": "^16.3.1",
		"env-schema": "^5.2.0",
		"fastify": "^4.19.2",
		"fastify-plugin": "^4.5.1"
	},
	"devDependencies": {
		"@types/node": "^20.4.1",
		"nodemon": "^3.0.1",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"tsx": "^4.19.1",
		"typescript": "^5.1.6"
	}
